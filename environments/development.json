{
  "name": "development",
  "override_attributes": {
    "algieba": {
      "mysql_users": [
        "development",
        "test"
      ],
      "open_ports": [
        "80:tcp",
        "3000:tcp"
      ]
    },
    "alterf": {
      "mysql_users": [
        "development",
        "test"
      ],
      "open_ports": [
        "80:tcp",
        "3000:tcp"
      ],
      "requirements": [
        "mysql-devel",
        "epel-release",
        "R"
      ]
    },
    "jenkins": {
      "deploy_jobs": [
        "algieba-deploy",
        "alterf-deploy",
        "chort-deploy",
        "denebola-deploy",
        "regulus-deploy",
        "zosma-deploy"
      ],
      "other_jobs": [
        "algieba-update-gems",
        "alterf-update-gems",
        "denebola-collect",
        "denebola-extract",
        "denebola-aggregate",
        "denebola-update-gems",
        "regulus-update-gems",
        "zosma-import-rates",
        "zosma-import-candle_sticks",
        "zosma-import-moving_averages",
        "zosma-compress",
        "zosma-update-gems"
      ],
      "polling_jobs": [
        "algieba",
        "alterf",
        "chort",
        "denebola",
        "regulus",
        "zosma"
      ],
      "views": [
        "payment_manager",
        "fx-rate_estimator",
        "horse-race_estimator"
      ]
    },
    "nginx": {
      "apps": [
        "algieba",
        "alterf",
        "regulus"
      ],
      "conf_files": [
        "doc",
        "jenkins"
      ]
    },
    "regulus": {
      "mysql_users": [
        "development",
        "test"
      ],
      "open_ports": [
        "80:tcp",
        "3000:tcp"
      ],
      "requirements": [
        "mysql-devel",
        "epel-release",
        "R"
      ]
    }
  }
}
