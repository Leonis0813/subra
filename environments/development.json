{
  "name": "development",
  "override_attributes": {
    "algieba": {
      "mysql_users": [
        "development",
        "test"
      ],
      "open_ports": [
        "80:tcp",
        "3000:tcp"
      ]
    },
    "alterf": {
      "mysql_users": [
        "development",
        "test"
      ],
      "open_ports": [
        "80:tcp",
        "3000:tcp"
      ],
      "requirements": [
        "cmake",
        "mysql-devel",
        "epel-release",
        "R"
      ]
    },
    "denebola": {
      "log_level": "debug"
    },
    "jenkins": {
      "job": {
        "check_pull_request": [
          "algieba-check-pull-request",
          "alterf-check-pull-request",
          "chort-check-pull-request",
          "denebola-check-pull-request",
          "regulus-check-pull-request",
          "subra-check-pull-request",
          "zosma-check-pull-request"
        ],
        "denebola": [
          "denebola-collect",
          "denebola-extract",
          "denebola-aggregate"
        ],
        "deploy": [
          "algieba-deploy",
          "alterf-deploy",
          "chort-deploy",
          "denebola-deploy",
          "regulus-deploy",
          "zosma-deploy"
        ],
        "polling": [
          "algieba",
          "alterf",
          "chort",
          "denebola",
          "regulus",
          "zosma"
        ],
        "update_gems": [
          "algieba-update-gems",
          "alterf-update-gems",
          "denebola-update-gems",
          "regulus-update-gems",
          "zosma-update-gems"
        ],
        "zosma": [
          "zosma-import-rates",
          "zosma-import-candle_sticks",
          "zosma-import-moving_averages",
          "zosma-compress"
        ]
      },
      "plugins": [
        "ansicolor",
        "git",
        "hidden-parameter",
        "htmlpublisher",
        "multiple-scms",
        "parameterized-trigger",
        "rebuild",
        "rubyMetrics",
        "workflow-aggregator"
      ],
      "views": [
        "payment_manager",
        "fx-rate_estimator",
        "horse-race_estimator"
      ]
    },
    "nginx": {
      "apps": [
        "algieba",
        "alterf",
        "regulus"
      ],
      "conf_files": [
        "doc",
        "jenkins"
      ]
    },
    "regulus": {
      "mysql_users": [
        "development",
        "test"
      ],
      "open_ports": [
        "80:tcp",
        "3000:tcp"
      ],
      "requirements": [
        "mysql-devel",
        "epel-release",
        "R"
      ]
    },
    "subra": {
      "branch": "master",
      "repository": "https://github.com/Leonis0813/subra.git",
      "ruby_version": "2.5.5"
    },
    "zosma": {
      "log_level": "debug"
    }
  }
}
