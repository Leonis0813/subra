{
  "name": "development",
  "override_attributes": {
    "algieba": {
      "mysql_users": [
        "development",
        "test"
      ]
    },
    "alterf": {
      "mysql_users": [
        "development",
        "test"
      ]
    },
    "denebola": {
      "log_level": "debug"
    },
    "jenkins": {
      "job": {
        "check_pull_request": [
          "algieba-check-pull-request",
          "alterf-check-pull-request",
          "chort-check-pull-request",
          "denebola-check-pull-request",
          "regulus-check-pull-request",
          "subra-check-pull-request",
          "zosma-check-pull-request"
        ],
        "denebola": [
          "denebola-collect",
          "denebola-extract",
          "denebola-aggregate"
        ],
        "deploy": [
          "algieba-deploy",
          "alterf-deploy",
          "chort-deploy",
          "denebola-deploy",
          "regulus-deploy",
          "zosma-deploy"
        ],
        "polling": [
          "algieba",
          "alterf",
          "chort",
          "denebola",
          "regulus",
          "zosma"
        ],
        "update_gems": [
          "algieba-update-gems",
          "alterf-update-gems",
          "denebola-update-gems",
          "regulus-update-gems",
          "zosma-update-gems"
        ],
        "zosma": [
          "zosma-import-rates",
          "zosma-import-candle_sticks",
          "zosma-import-moving_averages",
          "zosma-compress"
        ]
      },
      "plugins": [
        {
          "id": "ansicolor",
          "version": "0.5.2"
        },
        {
          "id": "git",
          "version": "3.9.1"
        },
        {
          "id": "hidden-parameter",
          "version": "0.0.4"
        },
        {
          "id": "htmlpublisher",
          "version": "1.18"
        },
        {
          "id": "multiple-scms",
          "version": "0.6"
        },
        {
          "id": "parameterized-trigger",
          "version": "2.35.2"
        },
        {
          "id": "rebuild",
          "version": "1.29"
        },
        {
          "id": "rubyMetrics",
          "version": "1.6.5"
        },
        {
          "id": "workflow-aggregator",
          "version": "2.6"
        }
      ],
      "views": [
        "payment_manager",
        "fx-rate_estimator",
        "horse-race_estimator"
      ]
    },
    "nginx": {
      "apps": [
        "algieba",
        "alterf",
        "regulus"
      ],
      "conf_files": [
        "doc",
        "jenkins"
      ]
    },
    "regulus": {
      "mysql_users": [
        "development",
        "test"
      ]
    },
    "subra": {
      "branch": "master",
      "repository": "https://github.com/Leonis0813/subra.git",
      "ruby_version": "2.5.5"
    },
    "zosma": {
      "log_level": "debug"
    }
  }
}
