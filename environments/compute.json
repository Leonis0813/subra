{
  "name": "compute",
  "override_attributes": {
    "alterf": {
      "mysql_users": [
        "production"
      ],
      "open_ports": [
        "80:tcp"
      ],
      "requirements": [
        "mysql-devel",
        "epel-release",
        "R"
      ]
    },
    "nginx": {
      "apps": [
        "alterf",
        "regulus"
      ],
      "conf_files": [
        "jenkins"
      ]
    },
    "regulus": {
      "mysql_users": [
        "production"
      ],
      "open_ports": [
        "80:tcp"
      ],
      "requirements": [
        "mysql-devel",
        "epel-release",
        "R"
      ]
    },
    "jenkins": {
      "deploy_jobs": [
        "alterf-deploy",
        "regulus-deploy"
      ],
      "plugins": [
        "git",
        "parameterized-trigger",
        "ansicolor",
        "rebuild",
        "workflow-aggregator",
        "hidden-parameter"
      ],
      "polling_jobs": [
        "alterf",
        "regulus"
      ],
      "views": [
        "fx-rate_estimator",
        "horse-race_estimator"
      ]
    }
  }
}
