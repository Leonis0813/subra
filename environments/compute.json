{
  "name": "compute",
  "override_attributes": {
    "alterf": {
      "mysql_users": [
        "production"
      ],
      "open_ports": [
        "80:tcp"
      ],
      "requirements": [
        "mysql-devel",
        "epel-release",
        "R"
      ]
    },
    "nginx": {
      "apps": [
        "alterf",
        "regulus"
      ],
      "conf_files": [
        "jenkins"
      ]
    },
    "regulus": {
      "mysql_users": [
        "production"
      ],
      "open_ports": [
        "80:tcp"
      ],
      "requirements": [
        "mysql-devel",
        "epel-release",
        "R"
      ]
    },
    "jenkins": {
      "jobs": [
        "alterf",
        "alterf-deploy",
        "regulus",
        "regulus-deploy"
      ]
    }
  }
}
