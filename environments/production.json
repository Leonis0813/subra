{
  "name": "production",
  "override_attributes": {
    "algieba": {
      "mysql_users": [
        "production"
      ]
    },
    "denebola": {
      "database": [
        {
          "env": "production",
          "name": "denebola"
        }
      ]
    },
    "jenkins": {
      "job": {
        "denebola": [
          "denebola-collect",
          "denebola-extract",
          "denebola-aggregate"
        ],
        "deploy": [
          "algieba-deploy",
          "chort-deploy",
          "denebola-deploy",
          "regulus-deploy",
          "zosma-deploy"
        ],
        "other": [
          "algieba-dump",
          "zosma-remove"
        ],
        "polling": [
          "algieba",
          "chort",
          "denebola",
          "regulus",
          "zosma"
        ],
        "zosma": [
          "zosma-import",
          "zosma-backup",
          "zosma-restore",
          "zosma-compress"
        ]
      },
      "plugins": [
        {
          "id": "ansicolor",
          "version": "0.5.2"
        },
        {
          "id": "build-blocker-plugin",
          "version": "1.7.3"
        },
        {
          "id": "git",
          "version": "3.9.1"
        },
        {
          "id": "hidden-parameter",
          "version": "0.0.4"
        },
        {
          "id": "parameterized-trigger",
          "version": "2.35.2"
        },
        {
          "id": "rebuild",
          "version": "1.29"
        },
        {
          "id": "workflow-aggregator",
          "version": "2.6"
        }
      ],
      "views": [
        "payment_manager",
        "fx-rate_estimator",
        "horse-race_estimator"
      ]
    },
    "nginx": {
      "apps": [
        "algieba",
        "redmine",
        "regulus"
      ],
      "conf_files": [
        "doc",
        "jenkins"
      ]
    },
    "regulus": {
      "mysql_users": [
        "production"
      ],
      "wait_latest": true
    }
  }
}
