  "override_attributes": {
    "algieba": {
      "open_ports": [
        "80:tcp"
      ],
      "mysql_users": [
        "production"
      ]
    },
    "jenkins": {
      "jobs": [
        "algieba",
        "algieba-deploy",
        "algieba-dump",
        "chort",
        "chort-deploy",
        "denebola",
        "denebola-deploy",
        "denebola-collect",
        "denebola-aggregate",
        "zosma",
        "zosma-deploy",
        "zosma-import-rates",
        "zosma-import-candle_sticks",
        "zosma-import-moving_averages",
        "zosma-compress",
        "zosma-remove"
