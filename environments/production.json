{
  "name": "production",
  "override_attributes": {
    "algieba": {
      "open_ports": ["80:tcp"],
      "mysql_users": ["production"]
    },
    "jenkins": {
      "jobs": [
        "alterf",
        "alterf-deploy",
        "regulus",
        "regulus-deploy",
        "regulus-aggregate",
        "algieba",
        "algieba-deploy",
        "chort"
      ]
    },
    "regulus": {
      "cron_settings": [
        {
          "name": "aggregate",
          "hour": "0",
          "minute": "0"
        },
        {
          "name": "aggregate/delete",
          "minute": "0"
        },
        {
          "name": "lib/compress",
          "weekday": "1",
          "hour": "0",
          "minute": "0"
        }
      ]
    }
  }
}
