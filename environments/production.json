{
  "name": "production",
  "override_attributes": {
    "algieba": {
      "open_ports": [
        "80:tcp"
      ],
      "mysql_users": [
        "production"
      ]
    },
    "denebola": {
      "database": [
        {
          "env": "production",
          "name": "denebola"
        }
      ]
    },
    "jenkins": {
      "job": {
        "denebola": [
          "denebola-collect",
          "denebola-extract",
          "denebola-aggregate"
        ],
        "deploy": [
          "algieba-deploy",
          "chort-deploy",
          "denebola-deploy",
          "regulus-deploy",
          "zosma-deploy"
        ],
        "other": [
          "algieba-dump",
          "zosma-remove"
        ],
        "polling": [
          "algieba",
          "chort",
          "denebola",
          "regulus",
          "zosma"
        ],
        "zosma": [
          "zosma-import-rates",
          "zosma-import-candle_sticks",
          "zosma-import-moving_averages",
          "zosma-compress"
        ]
      },
      "plugins": [
        "ansicolor",
        "git",
        "hidden-parameter",
        "parameterized-trigger",
        "rebuild",
        "workflow-aggregator"
      ],
      "views": [
        "payment_manager",
        "fx-rate_estimator",
        "horse-race_estimator"
      ]
    },
    "nginx": {
      "apps": [
        "algieba",
        "redmine"
      ],
      "conf_files": [
        "doc",
        "jenkins"
      ]
    }
  }
}
